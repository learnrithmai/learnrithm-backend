{
  "name": "learnrithm-backend",
  "main": "index.js",
  "author": "learnrithm-team",
  "private": true,
  "version": "2.0.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "dev": "tsx watch --env-file \".env.development.local\" src/server.ts",
    "build": "npm run clean && npx tsup src --format esm --out-dir dist --sourcemap false --external os --external path --external prisma",
    "start": "node dist/server.js",
    "start:watch": "tsx watch --env-file .env dist/server.js",
    "test": "vitest run || true",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ts": "tsx --test \"**/*.test.ts\"",
    "test:watch:ts": "tsx --test --watch \"**/*.test.ts\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix --quiet",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint-stages": "npm run lint && npm run type-check && npm run format",
    "prepare": "husky install",
    "clean": "rimraf dist",
    "zenstack:generate": "zenstack generate",
    "dev:prisma:migrate": "dotenv -e .env.development.local -- prisma migrate dev --skip-seed",
    "dev:prisma:db": "dotenv -e .env.development.local -- prisma db push",
    "dev:prisma:studio": "dotenv -e .env.development.local -- prisma studio",
    "dev:prisma:seed": "dotenv -e .env.development.local -- ts-node prisma/seed/seed.ts",
    "dev:prisma:reset": "dotenv -e .env.development.local -- prisma migrate reset",
    "dev:prisma:reset:force": "dotenv -e .env.development.local -- prisma migrate reset -f",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:db": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed/seed.ts",
    "prisma:reset": "prisma migrate reset",
    "prisma:reset:force": "prisma migrate reset -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learnrithmai/learnrithm-backend.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/learnrithmai/learnrithm-backend/issues"
  },
  "homepage": "https://github.com/learnrithmai/learnrithm-backend#readme",
  "description": "",
  "dependencies": {
    "@auth/core": "^0.37.4",
    "@auth/express": "^0.8.4",
    "@types/pino": "^7.0.4",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "chalk": "^4.1.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-zod-safe": "^1.3.3",
    "helmet": "^8.0.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.11.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-notifier": "^10.0.1",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^9.6.0",
    "rate-limiter-flexible": "^6.0.0",
    "safe-regex": "^2.1.1",
    "sha1": "^1.1.1",
    "supports-color": "^7.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^3.24.1",
    "zod_utilz": "^0.8.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@prisma/client": "^6.5.0",
    "@swc/core": "^1.11.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.1",
    "@types/node-notifier": "^8.0.5",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/safe-regex": "^1.1.6",
    "@types/sha1": "^1.1.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
